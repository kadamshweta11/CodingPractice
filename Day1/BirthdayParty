import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {

        // Write your code here
        Scanner sc=new Scanner(System.in);
        int T=sc.nextInt();
    for (int i=0;i<T;i++)
    {
        int n=sc.nextInt();
        int m=sc.nextInt();
        if(m%n==0)
        {
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }
    }
    }
}
