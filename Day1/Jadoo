
import java.util.*;

class TestClass {
    public static void main(String args[] ) throws Exception {
       
        //Scanner
        Scanner sc= new Scanner(System.in);
        // Write your code here
        int T=sc.nextInt();
        int arr[]=new int[T];
        int sum=0;
        for(int i=0;i<T;i++)
        {
           arr[i]=sc.nextInt();
           if(arr[i]>=0)
           {
               sum+=arr[i];
           }

        }
        System.out.println(sum);
    }
}
